@model IEnumerable<OdeToFood.Models.RestaurantReview>

@{
    ViewBag.Title = "Index";
}

@section featured {

<p>Showing the latest @Model.Count() reviews</p>

}

<h2>Index</h2>

@foreach(var item in Model) {
    @Html.Partial("_ReviewDiv", item)
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
